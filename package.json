{
    "name": "cypress-cucumber-webpack-example",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/node": "^10.17.56"
    },
    "scripts": {
        "test": "cypress run --spec \"**/*.feature\"",
        "test:all": "cypress run --spec \"**/*.features\"",
        "test:interactive": "npx cypress run --spec ./cypress/integration/interactiveBook/*.feature --browser chrome --env allure=true",
        "test:eemc": "npx cypress run --spec ./cypress/integration/EEMC/*.feature --browser chrome --env allure=true",
        "test:graduation": "npx cypress run --spec ./cypress/integration/graduationProject/*.feature --browser chrome --env allure=true",
        "test:course": "npx cypress run --spec ./cypress/integration/courseProject/*.feature --browser chrome --env allure=true"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^5.7.0",
        "@shelex/cypress-allure-plugin": "^2.5.1",
        "@types/cucumber": "^7.0.0",
        "cypress": "^6.8.0",
        "cypress-cucumber-preprocessor": "4.0.3",
        "cypress-file-upload": "^5.0.7",
        "cypress-iframe": "^1.0.1",
        "cypress-wait-until": "^1.7.1",
        "mocha": "^8.4.0",
        "mysql": "^2.18.1",
        "ts-loader": "^8.1.0",
        "typescript": "^4.2.3",
        "webpack": "^4.46.0"
    }
}